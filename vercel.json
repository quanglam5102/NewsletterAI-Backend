{
    "version": 2,
    "functions": {
      "api/**.py": {
        "maxDuration": 120
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "newsletter/wsgi.py",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }
  