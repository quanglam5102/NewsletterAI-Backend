{
    "version": 2,
    "builds": [
      {
        "src": "newsletter/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxDuration": 120
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "newsletter/wsgi.py",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }
  